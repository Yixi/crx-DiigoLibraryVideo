{
"name": "DiigoLibrary",
"version": "0.1",
"manifest_version":2,
"description": "let diigo library to show the vedio",
"minimum_chrome_version" : "6.0",
"icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"},
"permissions": ["contextMenus","unlimitedStorage", "tabs","bookmarks","history","cookies", "http://*/", "https://*/","chrome://favicon/"],
"content_scripts": [{
    "matches": ["http://*.diigo.com/*", "https://*.diigo.com/*"],
    "css": ["style/cs.css"],
    "js": ["js/jquery-1.8.0.min.js","js/contentscript.js"],
    "run_at": "document_end"
    }],
"background": {
      "page":"bg.html"
    }
}
